{
    "name": "midorikocak/arraytools",
    "type": "library",
    "description": "Handy array tools to creating and updating objects from arrays and converting objects to arrays and validating them.\n",
    "keywords": [
        "midorikocak",
        "array",
        "arrays",
        "object",
        "toarray",
        "toArray",
        "fromarray",
        "fromArray"
    ],
    "homepage": "https://github.com/midorikocak/arraytools",
    "license": "MIT",
    "authors": [
        {
            "name": "Midori Kocak",
            "email": "mtkocak@gmail.com",
            "homepage": "https://mtkocak.net",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "~7.4",
        "ext-json": "*",
        "midorikocak/querymaker": "^1.2.3",
        "ext-pdo": "*"
    },
    "require-dev": {
        "opsway/psr12-strict-coding-standard": "^0.3.0",
        "phpunit/phpunit": ">=8.0",
        "squizlabs/php_codesniffer": "^3.5",
        "ext-pdo": "*"
    },
    "autoload": {
        "psr-4": {
            "midorikocak\\arraytools\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "midorikocak\\arraytools\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs -vs src tests",
        "fix-style": "phpcbf src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
